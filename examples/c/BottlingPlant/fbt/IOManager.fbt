<BasicFB><InternalVars>
  <VarDeclaration Name="EmergencyStopped" Type="BOOL" Comment="" />
</InternalVars><ECC>
  <ECState Name="Start" Comment="" x="656.25" y="481.25">
    <ECAction Algorithm="IOAlgorithm" Output="EmergencyStopChanged" />
  </ECState>
  <ECTransition Source="Start" Destination="Start" Condition="true" x="656.25" y="1006.25" />
</ECC><Algorithm Name="IOAlgorithm" Comment="">
  <Other Language="C" Prototype="" Text="#define NUM_BOTTLES 4&#xD;&#xA;&#xD;&#xA;static int emergencyStopped = 1;&#xD;&#xA;&#xD;&#xA;static int conveyorSpeed = 0;&#xD;&#xA;&#xD;&#xA;int i;&#xD;&#xA;&#xD;&#xA;static int tickNum = 0;&#xD;&#xA;&#xD;&#xA;printf(&quot;ATTN: Tick number %i\n&quot;, tickNum);&#xD;&#xA;tickNum++;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;static int bottlePositions[NUM_BOTTLES] = {0};&#xD;&#xA;static int bottlesActive[NUM_BOTTLES] = {0};&#xD;&#xA;static int nextBottle = 0;&#xD;&#xA;&#xD;&#xA;//reset all the things&#xD;&#xA;me-&gt;EmergencyStop = 0;&#xD;&#xA;me-&gt;CanisterPressure = 255;&#xD;&#xA;me-&gt;FillContentsAvailable = 255;&#xD;&#xA;me-&gt;DoorSiteLaser = 0;&#xD;&#xA;me-&gt;InjectSiteLaser = 0;&#xD;&#xA;me-&gt;RejectSiteLaser = 0;&#xD;&#xA;me-&gt;RejectBinLaser = 0;&#xD;&#xA;me-&gt;AcceptBinLaser = 0;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;//printf(&quot;=====new tick\n&quot;);&#xD;&#xA;&#xD;&#xA;//continue progress&#xD;&#xA;if(conveyorSpeed) {&#xD;&#xA;	for(i = 0; i &lt; NUM_BOTTLES; i++) {&#xD;&#xA;		if(bottlesActive[i]) {&#xD;&#xA;			bottlePositions[i] += conveyorSpeed;&#xD;&#xA;			printf(&quot;IO: Canister %i moves to %i\n&quot;, i, bottlePositions[i]);&#xD;&#xA;			&#xD;&#xA;			if(bottlePositions[i] == 5) {&#xD;&#xA;				printf(&quot;IO: Canister %i at 5, triggering InjectSiteLaser\n&quot;, i);&#xD;&#xA;				me-&gt;_output.event.LasersChanged = 1;&#xD;&#xA;				me-&gt;InjectSiteLaser = 1;&#xD;&#xA;			}&#xD;&#xA;&#xD;&#xA;			if(bottlePositions[i] == 10) {&#xD;&#xA;				printf(&quot;IO: Canister %i at 10, triggering RejectSiteLaser\n&quot;, i);&#xD;&#xA;				me-&gt;_output.event.LasersChanged = 1;&#xD;&#xA;				me-&gt;RejectSiteLaser = 1;&#xD;&#xA;			}&#xD;&#xA;&#xD;&#xA;			if(bottlePositions[i] == 20) {&#xD;&#xA;				printf(&quot;IO: Canister %i at 20, falls off conveyor, triggering AcceptBinLaser\n&quot;, i);&#xD;&#xA;				me-&gt;_output.event.LasersChanged = 1;&#xD;&#xA;				me-&gt;AcceptBinLaser = 1;&#xD;&#xA;				bottlesActive[i] = 0;&#xD;&#xA;				bottlePositions[i] = 0;&#xD;&#xA;			}&#xD;&#xA;&#xD;&#xA;			if(me-&gt;_input.event.GoRejectArm &amp;&amp; (bottlePositions[i] == 10 || bottlePositions[i] == 11 || bottlePositions[i] == 12)) {&#xD;&#xA;				printf(&quot;IO: Go Reject Arm. Canister %i knocked from conveyor.\n&quot;, i);&#xD;&#xA;				//progress = 0;&#xD;&#xA;				me-&gt;_output.event.LasersChanged = 1;&#xD;&#xA;				me-&gt;RejectBinLaser = 1;&#xD;&#xA;				bottlesActive[i] = 0;&#xD;&#xA;				bottlePositions[i] = 0;&#xD;&#xA;			}&#xD;&#xA;		}&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(tickNum == 25) {&#xD;&#xA;	printf(&quot;Progress at 25, halting\n&quot;);&#xD;&#xA;	while(1);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;if(me-&gt;_input.event.InjectDone) {&#xD;&#xA;	printf(&quot;IO: Inject done\n&quot;);&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;if(emergencyStopped == 1) {&#xD;&#xA;	printf(&quot;IO: Releasing emergency stop\n&quot;);&#xD;&#xA;	me-&gt;_output.event.EmergencyStopChanged = 1;&#xD;&#xA;	me-&gt;EmergencyStop = 0;&#xD;&#xA;	emergencyStopped++;&#xD;&#xA;} else {&#xD;&#xA;	&#xD;&#xA;	if(me-&gt;_input.event.DoorReleaseCanister) {&#xD;&#xA;		printf(&quot;IO: Door released. Adding canister %i\n&quot;, nextBottle);&#xD;&#xA;&#xD;&#xA;		me-&gt;_output.event.LasersChanged = 1;&#xD;&#xA;		me-&gt;DoorSiteLaser = 1;&#xD;&#xA;			&#xD;&#xA;		bottlesActive[nextBottle] = 1;&#xD;&#xA;&#xD;&#xA;		nextBottle++;&#xD;&#xA;		nextBottle = nextBottle % NUM_BOTTLES;&#xD;&#xA;		&#xD;&#xA;	}&#xD;&#xA;	if(me-&gt;_input.event.InjectorPositionChanged) {&#xD;&#xA;		printf(&quot;IO: Injector position changed. Setting move finished.\n&quot;);&#xD;&#xA;		me-&gt;_output.event.InjectorArmFinishMovement = 1;&#xD;&#xA;	}&#xD;&#xA;&#xD;&#xA;	if(me-&gt;_input.event.ConveyorChanged) {&#xD;&#xA;		conveyorSpeed = me-&gt;ConveyorSpeed;&#xD;&#xA;		printf(&quot;IO: Setting conveyor movement to %i\n&quot;, conveyorSpeed);&#xD;&#xA;	}&#xD;&#xA;&#xD;&#xA;	&#xD;&#xA;&#xD;&#xA;	if(me-&gt;_input.event.InjectorControlsChanged) {&#xD;&#xA;		printf(&quot;IO: Injector controls changed. Now they are Vac: %1i Val: %1i Pmp: %1i\n&quot;, me-&gt;InjectorVacuumRun, me-&gt;InjectorContentsValveOpen, me-&gt;InjectorPressurePumpRun);&#xD;&#xA;		if(me-&gt;InjectorVacuumRun) {&#xD;&#xA;			printf(&quot;IO: Due to vacuum, changing canister pressure to 5.\n&quot;);&#xD;&#xA;			me-&gt;CanisterPressure = 5;&#xD;&#xA;			me-&gt;_output.event.CanisterPressureChanged = 1;&#xD;&#xA;		}&#xD;&#xA;		if(me-&gt;InjectorContentsValveOpen) {&#xD;&#xA;			printf(&quot;IO: Contents valve now open. Pressure changes slightly, sucking in contents.\n&quot;);&#xD;&#xA;			me-&gt;CanisterPressure = 20;&#xD;&#xA;			me-&gt;_output.event.CanisterPressureChanged = 1;&#xD;&#xA;		}&#xD;&#xA;		if(me-&gt;InjectorPressurePumpRun) {&#xD;&#xA;			printf(&quot;IO: Due to pressure pump, changing canister pressure to 250.\n&quot;);&#xD;&#xA;			me-&gt;CanisterPressure = 250;&#xD;&#xA;			me-&gt;_output.event.CanisterPressureChanged = 1;&#xD;&#xA;		}&#xD;&#xA;	}&#xD;&#xA;	&#xD;&#xA;	if(me-&gt;_input.event.FillContentsChanged) {&#xD;&#xA;		printf(&quot;IO: Fill contents changed.\n&quot;);&#xD;&#xA;	}&#xD;&#xA;&#xD;&#xA;	if(me-&gt;_input.event.StartVacuumTimer) {&#xD;&#xA;		printf(&quot;IO: Start vacuum timer.\n&quot;);//Elapsing timer.\n&quot;);&#xD;&#xA;		//me-&gt;_output.event.VacuumTimerElapsed = 1;&#xD;&#xA;	}&#xD;&#xA;&#xD;&#xA;	&#xD;&#xA;&#xD;&#xA;	if(me-&gt;_input.event.CanisterCountChanged) {&#xD;&#xA;		printf(&quot;IO: Canister count changed. New value: %i\n&quot;, me-&gt;CanisterCount);&#xD;&#xA;	}&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" />
</Algorithm></BasicFB>
</FBType>